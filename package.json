{
  "name": "soltone",
  "version": "1.1.1",
  "description": "soltone homepage",
  "main": "init.js",
  "author": "Yong",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/node": "^7.22.19",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.23.6",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "body-parser": "^1.20.2",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node": "^18.18.2",
    "postcss-loader": "^7.3.3",
    "pug": "^3.0.2",
    "sass": "^1.77.1",
    "sass-loader": "^13.3.2",
    "serve-favicon": "^2.5.0",
    "sitemap": "^7.1.1",
    "stream": "0.0.2",
    "typewriter-effect": "^2.21.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build && cp -R src/public build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build",
    "clean": "npm cache verify && rm -rf node_modules && rm -rf package-lock.json",
    "restart": "npm run clean && npm i && npm start --reset-cache",
    "gcp-build": ""
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  }
}
