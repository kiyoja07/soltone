{
  "name": "soltone",
  "version": "1.1.0",
  "description": "soltone homepage",
  "main": "init.js",
  "author": "Yong",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "airtable": "^0.10.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^4.2.0",
    "mini-css-extract-plugin": "^0.10.1",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "node": "^14.15.2",
    "node-sass": "4.14",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.0",
    "request": "^2.88.2",
    "sass-loader": "^7.3.1",
    "sitemap": "^6.4.0",
    "stream": "0.0.2",
    "typewriter-effect": "^2.17.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^4.2.0",
    "zlib": "^1.0.5"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build  && cp -R src/sitemap.xml build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1"
  }
}
